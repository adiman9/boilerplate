{
  "name": "webpack-basic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn webpack-dev-server -- --env.env=dev",
    "dev:analyse": "yarn dev -- --env.addons=bundleanalyse",
    "build": "yarn webpack -- --env.env=prod",
    "build:analyse": "yarn build -- --env.addons=bundleanalyse",
    "build:buddy": "yarn build -- --env.addons=bundlebuddy",
    "debug": "node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js",
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port=9001 --hot",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "chalk": "^2.0.1",
    "compression-webpack-plugin": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.19.0"
  }
}
