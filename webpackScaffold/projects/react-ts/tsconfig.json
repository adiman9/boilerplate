{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["es2015", "es2017", "dom"]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules",
    "./src/**/*.e2e.ts",
    "./src/**/*.spec.ts",
    "./config/protractor.config.ts"
  ]
}
